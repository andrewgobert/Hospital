<!DOCTYPE mapper PUBLIC '"-//mybatis.org//DTD Mapper 3.0//EN"         "org/apache/ibatis/builder/xml/mybatis-3-mapper.dtd"' 'null'>


<mapper namespace="com.mycompany.hospital.binary">
    
    <sql id="body">
        p.idpatiant as patiant
        p.fname as first
        p.lname as last
        p.address as address
        p.pcontact as pcontact
        p.scontact as scontact
        p.doctor as doctor
        p.nurse as nurse
        p.room as room
    </sql>
    
    <select id="getById" resultMap="patiantResultMap">
        SELECT
        <include refid="body"/>
        FROM
            patiants p
        WHERE id = #{id}
    </select>
    
    <resultMap id="patiantResultMap" type="com.solvd.course.db.binary.Car" autoMapping="false">
        <id column="patiant" property="idpatiant"/>
        <result column="first" property="fname"/>
        <result column="last" property="lname"/>
        <result column="address" property="address"/>
        <result column="pcontact" property="pcontact"/>
        <result column="scontact" property="scontact"/>
        <result column="doctor" property="doctor"/>
        <result column="nurse" property="nurse"/>
        <result column="room" property="room"/>
    </resultMap>
</mapper>

